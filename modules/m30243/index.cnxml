                <list id="id1172473995727" list-type="enumerated" number-style="arabic">
                  <item>Maak flitskaarte van die woorde:</item>
                </list>
              </entry>
              <entry>neus</entry>
              <entry>,</entry>
              <entry>ore</entry>
              <entry>,</entry>
              <entry>oë</entry>
              <entry>,</entry>
